{
  "name": "smartdental-ai",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd client && npm run dev",
    "dev:backend": "cd backend && uvicorn api.main:app --reload --port 8000",
    "install:all": "npm install && cd client && npm install && cd ../backend && pip install -r requirements.txt",
    "build": "cd client && npm run build",
    "lint": "cd client && npm run lint",
    "test": "cd client && npm run test",
    "format": "cd client && npm run format"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "axios": "^1.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "concurrently": "^8.0.1",
    "date-fns": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.1",
    "react-router-dom": "^6.11.1",
    "recharts": "^2.15.3",
    "swr": "^2.1.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.40.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.5.3",
    "prettier": "^2.8.8",
    "tailwindcss": "^4.1.4",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.0.4",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^4.2.0"
  }
