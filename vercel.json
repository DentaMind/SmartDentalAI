{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "JWT_SECRET": "@jwt_secret",
    "EMAIL_LINK_SECRET": "@email_link_secret",
    "OPENAI_API_KEY": "@openai_api_key",
    "REDIS_URL": "@redis_url"
  }
} 